{
  "name": "angular-socket",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "hmr": "ng serve --configuration hmr",
    "dev": "ng serve",
    "ngs": "ngsw-config dist/angular-socket ngsw-config.json /",
    "http": "http-server -p 8080 -c-1 dist/angular-socket",
    "dev:sw": "ng b --prod && http-server -p 8080 -c-1 dist/angular-socket",
    "exbuild": "ng b --prod=false --outputPath=../node-socket/public",
    "pBuild": "ng b --prod --outputPath=../node-socket/public",
    "start": "node server.js",
    "pretty": "prettier --write \"./**/*.ts\"",
    "prettys": "prettier --write",
    "heroku-postbuild": "ng build -c=production --aot",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.11",
    "@angular/cli": "^9.0.0-rc.10",
    "@angular/common": "~9.0.0-rc.11",
    "@angular/compiler": "~9.0.0-rc.11",
    "@angular/compiler-cli": "~9.0.0-rc.11",
    "@angular/core": "~9.0.0-rc.11",
    "@angular/forms": "~9.0.0-rc.11",
    "@angular/platform-browser": "~9.0.0-rc.11",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.11",
    "@angular/router": "~9.0.0-rc.11",
    "@angular/service-worker": "~9.0.0-rc.11",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@types/socket.io-client": "^1.4.32",
    "bootstrap": "4.3.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "ngx-bootstrap": "^5.3.2",
    "ngx-quicklink": "^0.1.5",
    "rxjs": "^6.5.4",
    "socket.io-client": "^2.3.0",
